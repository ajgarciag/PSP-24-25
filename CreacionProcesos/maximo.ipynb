    "l=input(\"Introduce numeros separados por comas: \")\n",
    "l=l.split(',')\n",
    "print(l)\n",
    "l=[int(x) for x in l]\n",
    "print(l)\n",
    "max=l[0]\n",
    "for i in range(len(l)):\n",
    "    if l[i]>max:\n",
    "        max=l[i]\n",
    "print(\"El maximo es \", max)"
